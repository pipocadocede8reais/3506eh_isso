fun main() {
   //La√ßos de repeti√ß√£o
   //while
   //Do...While
   //
   // for - para... 
   //
   // for -para cada item em uma lista/ ou qualquer tipo de cole√ß√£o, ser√° executado uma
   // determinada a√ß√£o
   //
   // Estrutura do for:
   // for (item in lista/colletion/intervalo){
   // a√ß√£o
   // }
   //
   /*
    for(item in 1..10){ 
       println(item)
    }
    */
    /*
    for(item in 1..1){ 
    println(3*1)
    println(3*2)
    println(3*3)
    println(3*4)
    println(3*5)
    println(3*6)
    println(3*7)
    println(3*8)
    println(3*9)
    println(3*10)
    
    }
    */
    //
    // Repeat - repeti√ß√£o
    // uma determinada
    // 
   var tartaruga = 1  
    repeat(10){ 
    println(tartaruga)
    tartaruga++
    }
}
